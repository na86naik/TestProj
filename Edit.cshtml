@model IEnumerable<FitnessAchieve.Models.FitnessCoachList>

@{
    ViewBag.Title = "FitnessCoachList";
    Layout = "~/Views/Shared/_Layout.cshtml";
    WebGrid grid = new WebGrid(Model);

}

<h2>Index</h2>

<div>
    <h4>FitnessCoachList</h4>
    <hr />
    <div id="gridImages">
        @grid.GetHtml(tableStyle: "webGrid",
            headerStyle: "header",
            displayHeader: true,
            alternatingRowStyle: "alt",
            selectedRowStyle: "select",
            mode: WebGridPagerModes.All,
            columns: grid.Columns(
        //grid.Column("Select", format: @<text>@Html.RadioButton("rbFile", "", false) </text>),
        grid.Column(columnName: "Select", format: (item) => Html.ActionLink("Select", "Edit", new { id = item.CoachId })),
        grid.Column("CoachName"),
        grid.Column("CoachExpertise"),
        grid.Column("ContactNo")

        ))
    </div>
</div>
